process_name=udene
process_path=ngolem

# PDG codes
# u: 2, d: 1
# Gluon: 21
# Z-Boson: 23
# e-: 11, \nu_e: 12
in=-2, 1
out=11, -12

order=QCD, 0, 2

model=sm

# Only W+ in s-channel, no Goldstone bosons
qgraf.verbatim=\
true=bridge[Wp,Wm,1,100];

qgraf.options=onshell,notadpole,nosnail

one=gs,e
zero=mU,mD,me

# There's only one non-zero helicity
helicities=+--+

extensions=dred
